                            android:weightSum="3">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:orientation="vertical"
                                android:layout_weight="2.5"
                                android:background="@drawable/buttonshape_find_location_search_white"
                                android:gravity="center_vertical"
                                android:paddingTop="5dp"
                                android:paddingBottom="5dp">
                                <Spinner
                                    android:id="@+id/spn_tow"
                                    android:layout_width="match_parent"
                                    android:textSize="@dimen/font_18"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="match_parent"/>
                            </LinearLayout>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:gravity="center_horizontal|center_vertical"
                                android:text="พ่วง"
                                android:textSize="@dimen/font_18"
                                android:textStyle="bold"
                                android:layout_weight="0.5"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginLeft="30dp"
                            android:layout_marginRight="30dp"
                            android:layout_marginTop="5dp"
                            android:weightSum="3">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:orientation="vertical"
                                android:layout_weight="2.5"
                                android:background="@drawable/buttonshape_find_location_search_white"
                                android:gravity="center_vertical"
                                android:paddingTop="5dp"
                                android:paddingBottom="5dp">
                                <EditText
                                    android:id="@+id/edt_sum1"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:ems="10"
                                    android:hint="กรุณาใส่จำนวน"
                                    android:gravity="center_horizontal|center_vertical"
                                    android:textAlignment="center"
                                    android:textSize="@dimen/font_18"
                                    android:textColor="@color/black_1"
                                    android:inputType="numberDecimal"
                                    android:maxLength="2"
                                    android:enabled="false"
                                    android:background="@null"/>
                                <Spinner
                                    android:id="@+id/spn_sum1"
                                    android:layout_width="match_parent"
                                    android:textSize="@dimen/font_18"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="match_parent"
                                    android:visibility="gone"/>
                            </LinearLayout>
                            <TextView
                                android:id="@+id/txt_sum"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:gravity="center_horizontal|center_vertical"
                                android:text="ล้อ"
                                android:textSize="@dimen/font_18"
                                android:textStyle="bold"
                                android:layout_weight="0.5"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginLeft="30dp"
                            android:layout_marginRight="30dp"
                            android:layout_marginTop="5dp"
                            android:weightSum="3">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:orientation="vertical"
                                android:layout_weight="2.5"
                                android:background="@drawable/buttonshape_find_location_search_white"
                                android:gravity="center_vertical"
                                android:paddingTop="5dp"
                                android:paddingBottom="5dp">
                                <EditText
                                    android:id="@+id/edt_sum2"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:ems="10"
                                    android:hint="กรุณาใส่จำนวน"
                                    android:gravity="center_horizontal|center_vertical"
                                    android:textAlignment="center"
                                    android:textSize="@dimen/font_18"
                                    android:textColor="@color/black_1"
                                    android:inputType="numberDecimal"
                                    android:maxLength="3"
                                    android:enabled="false"
                                    android:background="@null"/>
                                <Spinner
                                    android:id="@+id/spn_weight"
                                    android:layout_width="match_parent"
                                    android:textSize="@dimen/font_18"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="match_parent"
                                    android:visibility="gone"/>
                            </LinearLayout>
                            <TextView
                                android:id="@+id/txt_weight"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:gravity="center_horizontal|center_vertical"
                                android:text="ตัน"
                                android:textSize="@dimen/font_18"
                                android:textStyle="bold"
                                android:layout_weight="0.5"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="30dp"
                            android:layout_marginRight="30dp"
                            android:layout_marginTop="5dp"
                            android:paddingTop="5dp"
                            android:paddingBottom="5dp">
                            <CheckBox
                                android:id="@+id/chk_weight"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:enabled="false"
                                android:text="ไม่รวมน้ำหนัก"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linear_option"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="30dp"
                            android:layout_marginRight="30dp"
                            android:layout_marginTop="5dp"
                            android:paddingTop="5dp"
                            android:paddingBottom="5dp"
                            android:visibility="visible">
                            <CheckBox
                                android:id="@+id/chk_option"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:enabled="false"
                                android:text="พื้นเรียบ"/>
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="vertical"
                        android:background="@drawable/buttonshape_find_location_search_white"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="30dp"
                        android:layout_marginRight="30dp"
                        android:layout_marginTop="5dp"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:visibility="gone">
                        <Spinner
                            android:id="@+id/spn_geo"
                            android:layout_width="match_parent"
                            android:textSize="@dimen/font_18"
                            android:layout_gravity="center_vertical"
                            android:layout_height="match_parent"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="vertical"
                        android:background="@drawable/buttonshape_find_location_search_white"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="30dp"
                        android:layout_marginRight="30dp"
                        android:layout_marginTop="5dp"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:visibility="gone">
                        <Spinner
                            android:id="@+id/spn_province"
                            android:layout_width="match_parent"
                            android:textSize="@dimen/font_18"
                            android:layout_gravity="center_vertical"
                            android:layout_height="match_parent"/>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</RelativeLayout>
